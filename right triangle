import java.util.Scanner;
public class Pattern
{
    public static void main(String []args)
    {
        int i,j;
        Scanner scanner = new Scanner(System.in);
        int n1= scanner.nextInt();
        for (i=1;i<=n1;i++)
        {
            for (j=1;j<=i;j++)
            {
                System.out.print("*");
            }
                System.out.println(" ");
        }
}
}


out put:
*
* *
* * *
* * * *
