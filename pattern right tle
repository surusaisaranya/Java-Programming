import java.util.Scanner;
class pattern
{
    public static void main(String[] args)
    {
        Scanner scanner = new Scanner (System.in);
        System.out.println("enter the number odf times to get printed");
        int n=scanner.nextInt();
        for (int i=1;i<=n;i++)
        {
            for (int j=1;j<=i;j++)
            {
                System.out.print("%");
            }
            System.out.println();
        }
    }
}

output:
enter the number odf times to get printed
3
