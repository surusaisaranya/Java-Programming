import java.util.Scanner;
class Factorial
{
    public static void main(String[] args)
    {
        Scanner scanner = new Scanner(System.in);
        System.out.println("enter the number ");
        int n=scanner.nextInt();
        int fact=1;
        for (int i=1;i<=n;i++)
        {
            fact=fact*i;
        }
        System.out.println("factorial is"+fact);
    }
}

output:
enter the number 
124
factorial is0
